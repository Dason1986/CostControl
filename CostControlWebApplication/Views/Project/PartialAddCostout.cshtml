
@{


}
<style>
    .box-card {
        width: 100%;
    }
</style>
<script type="text/x-template" id="addcostout">

    <el-dialog width="80%"
               title="成本支出"
               :visible.sync="eldialogvisible"
               append-to-body>
        <span slot="footer" class="dialog-footer">
            <el-button @@click="eldialogvisible = false">取 消</el-button>
            <el-button type="danger" @@click="onSumbit()">保存</el-button>
        </span>
        <el-container>
            <el-main>

                <el-card class="box-card">


                     

                    <div class="row">

                        <div class="col-md-3 form-inline">
                            

                                <div class="form-group">
                                    <label>單號</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="單號"></el-input>
                                </div>

                        

                        </div>
                        <div class="col-md-3 form-inline">
                            
                                <div class="form-group">
                                    <label>類別</label>
                                    <el-select placeholder="類別" v-model="rec.type" filterable>
                                    </el-select>
                                </div>

                             

                        </div>
                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>日期</label>
                                    <el-date-picker type="date" v-model="rec.createDate" format="yyyy-MM-dd" value-format="yyyy-MM-dd" placeholder="日期">
                                    </el-date-picker>
                                </div>

                         

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>收入支出單位</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="收入支出單位"></el-input>
                                </div>

                         

                        </div>
                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>摘要</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="摘要"></el-input>
                                </div>

                          

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>幣種</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="幣種"></el-input>
                                </div>
 

                        </div>
                        <div class="col-md-3 form-inline">
                        
                                <div class="form-group">
                                    <label>借方金額（收）</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="借方金額（收）"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>貸方金額（支）</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="貸方金額（支）"></el-input>
                                </div>
                             

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>發票號</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="發票號"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>結算方式</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="結算方式"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>支票號</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="支票號"></el-input>
                                </div>
                             

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>開票日期</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="開票日期"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>轉數/入數</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="轉數/入數"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>預計付款日期</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="預計付款日期"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>收/付款跟進人</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="收/付款跟進人"></el-input>
                                </div>
                             

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-3 form-inline">
                        
                                <div class="form-group">
                                    <label>調撥單號</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="調撥單號"></el-input>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>兌票日期</label>
                                    <el-input type="text"v-model="rec.orderNo" placeholder="兌票日期"></el-input>
                                </div>
 

                        </div>
                        <div class="col-md-3 form-inline">
                          
                                <div class="form-group">
                                    <label>會計確認</label>
                                    <el-switch type="text"v-model="rec.orderNo" placeholder="會計確認"></el-switch>
                                </div>
                             

                        </div>
                        <div class="col-md-3 form-inline">
                           
                                <div class="form-group">
                                    <label>單據是否交接</label>
                                    <el-switch type="text"v-model="rec.orderNo" placeholder="單據是否交接"></el-switch>
                                </div>
                             

                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-12 form-inline">
                            <div class="form-group">

                                <label>備註</label>
                                <el-input type="textarea"
                                          :autosize="{ minRows: 3, maxRows: 7}" style="width: 700px;"
                                          placeholder="備註"
                                          v-model="rec.remark">
                                </el-input>
                            </div>

                        </div>
                    </div>
                </el-card>




            </el-main>
        </el-container>
    </el-dialog>


</script>

<script type="text/javascript">


    Vue.component('el-add-costout',
        {

            template: '#addcostout',
            data: function () {
                return {
                    eldialogvisible: this.visible,

                    rec: {}
                };
            },
            filters: {

            },
            methods: {



                onSumbit: async function () {

                    await this.$http.post("/api/BatchPackage/CompareResult/Approve/" + compareResult.id);
                    this.eldialogvisible = false;
                    this.$emit('closed', true);

                },



            },
            updated: function () {

            },
            watch: {
                visible: function (val) {


                    this.eldialogvisible = val;
                },
                eldialogvisible: function (val) {

                    if (val) return;

                    this.$emit('update:visible', val)
                }

            },
            props: {

                projectid: String,
                visible: Boolean
            }
        });


</script>
